{
    "presets":[
        [
            "@babel/preset-env",
            {
                "loose": true,
                "modules": false,
                "useBuiltIns": "entry"
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "absoluteRuntime": false,
                "corejs": false,
                "helpers": true,
                "regenerator": true,
                "useESModules": false
            }
        ],
        "@babel/plugin-proposal-class-properties"
    ]
}